.foot{
  width:100%;
  height:100rpx;
  background:#131D26;
  display: flex;
  position:relative;
}
.quan{
  width:100rpx;
  background:#131D26;
  height:100rpx;
  color:white;
  position:absolute;
  bottom: 0;
  left:40rpx;
  border-radius: 90%;
  padding:20rpx;
}
.quan>view{
  background:#2B343C;
  width:95rpx;
  height:95rpx;
  border-radius: 50%;
  text-align: center;

}
.foot>view:nth-of-type(2){
  color:#919396;
  font-weight: bold;
  margin-left:230rpx;
  line-height: 100rpx;
  font-size:40rpx;
}
.foot>view:nth-of-type(3){
  font-size:27rpx;
  color:#919396;
  margin-left:30rpx;
  line-height: 90rpx;
}
.foot>view:nth-of-type(4){
  width:24%;
  height:100%;
  background:#2B343C;
  /* float:right; */
  margin-left:46rpx;
  line-height:100rpx;
  font-weight: bold;
  color:#979A9C;
  font-size:26rpx;
  text-align: center;
}

