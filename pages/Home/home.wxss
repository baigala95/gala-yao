/* pages/Home/home.wxss */
@import "../../Components/goods/good";
@import "../../Components/com/com";

.home{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  /* position:relative; */
}
.isRuleShow {
  display: block;
}

.isRuleHide {
  display: none;
}
.float{
  width:100%;
  height:100%;
  border:1rpx solid darkcyan;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 2;
  top: 0;
  left: 0;
  color:white;
}
.float>view:nth-of-type(1){
  font-size:32rpx;
  color:white;
  font-weight: bold;
  width:100%;
  text-align: center;
  margin-top:100rpx;
}
.star{
  width:100%;
  text-align: center;
  margin-top:20rpx;
}
.you{
  width:100%;
  text-align: center;
  font-size:28rpx;
  font-weight: bold;
  margin-top:50rpx;
}
.list{
  width:100%;
  margin-left:100rpx;
  font-size:24rpx;
  font-weight: 200;
  line-height:40rpx;
}
.shang{
  font-size:32rpx;
  font-weight: bold;
  color:white;
  text-align: center;
  width:100%;
  margin-top:30rpx;
}
.text{
  width:100%;
  margin-left:100rpx;
  font-size:24rpx;
  line-height:40rpx;
  margin-right:100rpx;
  width:500rpx;
  margin-top:50rpx;
  font-weight:200;
}
.no{
  width:100%;
  text-align: center;
  font-size:64rpx;
  font-weight: bold;
  display: block;
  margin-top:100rpx;
}



.five{
  background:#4D5055;
  padding:10rpx 20rpx;
  border-radius: 30rpx;
  font-size:20rpx;
  color:white;
  margin-left:140rpx;
}


.main{
  flex: 1;
  overflow: auto;
}
.home-top{
  width:100%;
  height:250rpx;
  background:rgba(0, 0, 0, 0.2);
  display: flex;
  position:relative;
  
}
.ban{
  width:100%;
  height:80rpx;
  display: flex;
  border:1rpx solid darkgray;
}
.top-right{
  padding-top:48rpx;
  padding-left:32rpx;
}
.one>text:nth-of-type(1){
  display: inline-block;
  width:65rpx;
  height:40rpx;
  background:#F01414;
  color:white;
  border-radius: 7%;
  font-size:30rpx;
  font-weight: bold;
  padding:2rpx;
}
.one>text:nth-of-type(2){
  font-size:32rpx;
  font-weight: bold;
  color:white;
  margin-left:20rpx;
}
.two{
  color:#EEEEEF;
}
.three>text:nth-of-type(1){
  display: inline-block;
  width:30rpx;
  height:30rpx;
  background:white;
  color:#F67272;
  font-size:25rpx;
  padding:2rpx;
  border-radius: 5rpx;
}
.three>text:nth-of-type(2){
  color:#EEEEEF;
  font-size:28rpx;
  margin-right:5rpx;
}
.top-down{
  box-sizing: border-box;
  position:absolute;
  bottom: 0;
  left:0;
  width:100%;
  height:56rpx;
  background:#4D5359;
  padding-left:24rpx;
  display: flex;
}
.top-down>text:nth-of-type(1){
  display: inline-block;
  background:white;
  font-size:20rpx;
  padding: 3rpx 7rpx;
  border-radius: 10rpx;
  color:#4D5359;
  height:35rpx;
  margin-top:10rpx;

}
.top-down>text:nth-of-type(2){
display:inline-block ;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;  
width:75%;
color:white;
margin-top:10rpx;
margin-left:20rpx;
}


.swiper-tab{
 width: 100%;
 border-bottom: 2rpx solid #777777;
 text-align: center;
 line-height: 80rpx;
}

.swiper-tab-list{
 font-size: 30rpx;
 display: inline-block;
 width: 20%;
 color: #777777;
 margin-right: 4%;
 margin-left: 4%;
}

.on{
 color: firebrick;
 /* border-bottom: 5rpx solid #da7c0c; */
}

.swiper-box{
 display: block;
 height: 750rpx !important;
 width: 100%;
 overflow: hidden;
}

